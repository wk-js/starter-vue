       url(asset_url_string('assets/fonts/asset_url_string.eot?#iefix')) format('embedded-opentype'), /* IE6-IE8 */
       url(asset_url_string('assets/fonts/asset_url_string.woff2')) format('woff2'), /* Super Modern Browsers */
       url(asset_url_string('assets/fonts/asset_url_string.woff')) format('woff'), /* Modern Browsers */
       url(asset_url_string('assets/fonts/asset_url_string.ttf')) format('truetype'), /* Safari, Android, iOS */
       url(asset_url_string('assets/fonts/asset_url_string.svg#Scheherazade')) format('svg'); /* Legacy iOS */
}